#!/usr/bin/env bash


prefix="confluent_$2"

if [[ "confluent_ekg_elasticsearch_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_ekg_elasticsearch_volume
	fi
	
if [[ "confluent_ekg_grafana_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_ekg_grafana_volume
	fi
	
if [[ "confluent_ingest_data_filebeat_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_ingest_data_filebeat_volume
	fi
	
if [[ "confluent_kz_kafka_secrets_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_kz_kafka_secrets_volume
	fi
	
if [[ "confluent_kz_kafka_data_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_kz_kafka_data_volume
	fi
	
if [[ "confluent_kz_zookeeper_secrets_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_kz_zookeeper_secrets_volume
	fi
	
if [[ "confluent_kz_zookeeper_data_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_kz_zookeeper_data_volume
	fi
	
if [[ "confluent_kz_zookeeper_log_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_kz_zookeeper_log_volume
	fi
	
if [[ "confluent_metrics_overlay_metrics_overlay_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_metrics_overlay_metrics_overlay_volume
	fi
	
if [[ "confluent_processor_streamsets_volume" == "$prefix"* ]] ; then
	executionplane --silent docker volume create confluent_processor_streamsets_volume
	fi
	
